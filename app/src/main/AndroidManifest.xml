<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.github.zqhcxy.glidetest"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.READ_SMS"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.NotificationTargetActivity">
        </activity>
        <activity android:name=".activity.BlurImageActivity">
        </activity>
        <activity android:name=".activity.TransformationsActivity">
        </activity>
        <activity android:name=".activity.AnimateActivity">
        </activity>
        <activity android:name=".activity.GlideLoadActivity">
        </activity>
    </application>

</manifest>